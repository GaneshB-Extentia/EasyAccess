<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >
   
	<LinearLayout 
	    android:orientation="vertical"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content" 
	    android:background="@color/homescreen_background" 
	    android:layout_weight="1"> 
	    
		<TableLayout
		    android:gravity="center_vertical"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:layout_centerVertical="true"
		   	android:stretchColumns="0" 
		    android:paddingLeft="12px" 
			android:paddingRight="12px"
			android:paddingTop="12px" > 
			
			<TableRow
			    android:layout_weight="1" 
			    android:gravity="center_vertical" >
			   
				<Button  
					android:id="@+id/btnAlarmSet"
					android:text="@string/btnAlarmSet"
					android:contentDescription="@string/btnAlarmSet"
					android:layout_weight="1"
				    android:layout_width="fill_parent" 
				    android:layout_height="match_parent" 
				    android:background="@drawable/card"
				    android:textColor="@drawable/card_textcolor" 
				    android:textStyle="bold"
				    android:textSize="@dimen/card_textsize_regular"
				    android:gravity="center"
					android:focusable="true"
				/>	
			</TableRow> 
			
			<TableRow
			    android:layout_weight="0" 
			    android:gravity="center_vertical" >
		
				<TextView  
					android:id="@+id/txtEmpty2"
				    android:text="@string/txtEmpty"			
				    android:layout_width="1px" 
				    android:layout_height="12px"
				    android:layout_span="1"
				    android:background="@color/homescreen_background"
				    android:focusable="false"
				/>			
			</TableRow>
		
			<TableRow
			    android:layout_weight="1" 
			    android:gravity="center_vertical" >
			   
				<TextView  
					android:id="@+id/txtAlarmTimeUserInput"
					android:text="@string/txtAlarmTimeUserInput"
					android:contentDescription="@string/txtAlarmTimeUserInput"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
				    android:background="@color/homescreen_background"	
				    android:textColor="@color/card_textcolor_regular"
				    android:textSize="@dimen/card_textsize_phonedialer"
				    android:gravity="right"	    
				    android:focusable="false"
				    android:ellipsize="none"
				    android:scrollHorizontally="false"
				/>				
				
				<TextView  
					android:id="@+id/txtEmpty1"
					android:text="@string/txtEmpty"
				    android:layout_width="12px" 
				    android:layout_height="1px"
				    android:background="@color/homescreen_background"		    
				    android:focusable="false"
				/>					
				
