					android:id="@+id/btnKeypadBackspace"
					android:text="@string/btnKeypadBackspace"
					android:contentDescription="@string/btnKeypadBackspaceTalkBackFriendly"
				    android:layout_width="@dimen/keypad_backspace_width"
